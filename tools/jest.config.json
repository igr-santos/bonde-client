{
  "testPathDirs": ["test"],
  "moduleFileExtensions": ["js", "jsx"],
  "moduleDirectories": ["node_modules"],
  "moduleNameMapper": {
    "^~components(.*)$": "<rootDir>/client/components$1",
    "^~util(.*)$": "<rootDir>/client/util$1",

    "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/test/mock-file.js",
    "\\.(css|less|scss)$": "<rootDir>/test/mock-style.js"
  }
}

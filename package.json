{
  "name": "hub-client",
  "version": "1.0.0",
  "description": "This is the Our Cities Hub client project",
  "author": "",
  "license": "ISC",
  "homepage": "https://github.com/meurio/hub-client#readme",
  "engines": {
    "node": "5.7.1",
    "npm": "3.6.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/meurio/hub-client"
  },
  "bugs": {
    "url": "https://github.com/meurio/hub-client/issues"
  },
  "scripts": {
    "build": "webpack --verbose --colors --display-error-details --config webpack/prod.config.js",
    "watch-client": "node ./node_modules/better-npm-run watch-client",
    "start-dev": "node ./node_modules/better-npm-run start-dev",
    "dev": "concurrently --kill-others \"npm run watch-client\" \"npm run start-dev\"",
    "test": "./node_modules/.bin/karma start --log-level debug",
    "postinstall": "npm run build"
  },
  "betterScripts": {
    "start-dev": {
      "command": "node ./bin/server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "development",
        "API_URL": "http://localhost:3000",
        "APP_DOMAIN": "localhost.dev:3001"
      }
    },
    "watch-client": {
      "command": "node webpack/webpack-dev-server.js",
      "env": {
        "UV_THREADPOOL_SIZE": 100,
        "NODE_PATH": "./src",
        "API_URL": "http://localhost:3000",
        "APP_DOMAIN": "localhost.dev:3001"
      }
    }
  },
